h2
  margin-bottom 30px
.storages
  display flex
  flex-direction row
  justify-content center
  align-items center
  .storage
    position relative
    .check-badge
      position absolute
      color rgb(0, 128, 0)
      font-size 30px
      z-index 1000
      right 0
      top -10px
    .storage-logo
      width 100px
      height 100px
      background-size contain
      background-repeat no-repeat
      cursor pointer
      &.accessed
        opacity 0.6
      &.google-logo
        background-image inline-url("./google.svg","utf8")
